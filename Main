#include <iostream>
#include <Map.h>
#include <Player.h>
#include <Inventory.h>

using namespace std;




void keyInput(unsigned char key)
{
   switch(key)
   {
       case w://w key
       case W://W key
       case 72://up arrow
       
           break;
       case a://a key
       case A://A key
       case 75://left arrow
       
           break;
       case s://s key
       case S://S key
       case 80://down arrow
       
           break;
       case d://d key
       case D://D key
       case 77://right arrow
       
           break;
       default:
           cout << "you can't move that way" << endl;
           break;
   }
}

int main ()
{
    
}
